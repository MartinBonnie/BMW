.upload_photo .top strong{ display:block; color:#333333; font-size:14px; padding-bottom:5px}

.upload_photo .top p{ float:right; color:#999999; padding-top:10px;}
.upload_photo table{ width:100%; border:1px #cccccc solid; line-height:24px; background:#fff}
.upload_photo table thead{ background:#e5f0ff}
.upload_photo table th{ border-bottom:1px #d8d8d8 solid; font-weight:normal}
.upload_photo table .s1{ width:30px; text-align:center}
.upload_photo table .s2{ }
.upload_photo table .s2 span{ color:#333}
.upload_photo table .s2 span.qd{ color:#aaaaaa}
.upload_photo table .s2 span.wai{ border:1px #aaaaaa solid; color:#ffffff; float:left; margin:5px 5px 0 0; *margin:3px 0 0 0}
.upload_photo table .s2 span.wai input{ border-top:1px #dddddd solid;border-left:1px #dddddd solid; float:left; width:210px}
.upload_photo table .s2 a.qx{ color:#999999; text-decoration:none}
.upload_photo table .s2 a{ margin-right:5px; text-decoration:underline} 
.upload_photo table .s3{ width:170px;}
.upload_photo table .s4{ width:60px; text-align:center}
.upload_photo table .s4 a{ text-decoration:underline}
.upload_photo table .s4 a.del{ color:#FF0000}
.upload_photo table .s4 em{ color:#339900; font-style:normal }
.upload_photo table .s4 em.true{ width:9px; height:8px; background:url(../images/upload.png) no-repeat 0 -61px; display:block; margin:0 auto; font-size:0 }
.upload_photo table .s4 em.false{ width:9px; height:8px; background:url(../images/upload.png) no-repeat -10px -61px; display:block; margin:0 auto; font-size:0 }
.upload_photo table .s4 i{}

.upload_photo table td{ line-height:30px; border-bottom:1px #e7e7e7 solid}
